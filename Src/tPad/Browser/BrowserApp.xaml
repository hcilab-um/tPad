<UserControl x:Class="UofM.HCI.tPad.App.Browser.BrowserApp"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:my="http://schemas.awesomium.com/winfx"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" >
  <DockPanel>
    <DockPanel DockPanel.Dock="Bottom">
      <Button x:Name="btnClose" Content="Close" Height="30" Padding="10,0,10,0" DockPanel.Dock="Left"/>
      <Button x:Name="btnGo" Content="Go" Height="30" Padding="10,0,10,0" DockPanel.Dock="Right"/>
      <TextBox x:Name="tbAddress"/>
    </DockPanel>
    <my:WebControl Name="webControl1" Source="http://www.wikiedia.org/"/>
  </DockPanel>
</UserControl>
